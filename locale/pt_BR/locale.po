msgstr "Download XML"

msgid "plugins.importexport.crossrefConference.action.markRegistered"
msgstr "Marcar como Ativo"

msgid "plugins.importexport.crossrefConference.senderTask.name"
msgstr "Dados CrossRef do remetente"

msgid "plugins.importexport.crossrefConference.cliUsage"
msgstr ""
"Uso:\n"
"{$scriptName} {$pluginName} export [xmlFileName] [journal_path] articles objectId1 [objectId2] ...\n"
"{$scriptName} {$pluginName} register [journal_path] articles objectId1 [objectId2] ...\n"
""

msgid "plugins.importexport.crossrefConference.export.error.issueNotFound"
msgstr "Nenhuma edição correspondeu ao ID de edição especificado \"{$issueId}\"."

msgid "plugins.importexport.crossrefConference.export.error.articleNotFound"
msgstr "Nenhum artigo correspondeu ao ID do artigo especificado \"{$articleId}\"."

msgid "plugins.importexport.crossrefConference.register.success.warning"
msgstr "O registro foi bem-sucedido, mas ocorreu o seguinte aviso: '{$param}'."

msgid "plugins.importexport.crossrefConference.register.error.mdsError"
msgstr ""
"O registro não foi totalmente bem-sucedido! O servidor de registro DOI "
"retornou um erro."

msgid "plugins.importexport.crossrefConference.action.register"
msgstr "Depositar"

msgid "plugins.importexport.crossrefConference.settings.form.validation"
msgstr ""
"Validar XML. Use esta opção para o download do XML para o registro manual do "
"DOI."

msgid "plugins.importexport.crossrefConference.settings.form.onlyValidateExport"
msgstr "Validar apenas a exportação. Não baixar o arquivo."

msgid "plugins.importexport.crossrefConference.settings.form.conferenceNameOptionIssueTitle"
msgstr "Usar título da edição"

msgid "plugins.importexport.crossrefConference.settings.form.conferenceNameOptionCustom"
msgstr "Inserir nome da conferência"